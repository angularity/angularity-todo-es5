{
  "name": "angularity-todo-es5",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "node-bourbon": "1.2.2"
  },
  "bundledDependencies": [
    "examplelib"
  ],
  "scripts": {
    "build": "webpack -d --progress",
    "build-unminified": "cross-env ANGULARITYTODOES5_NO_MINIFY=true webpack -d --progress",
    "watch": "webpack -d --watch",
    "watch-unminified": "cross-env ANGULARITYTODOES5_NO_MINIFY=true webpack -d --watch",
    "release": "cross-env ANGULARITYTODOES5_RELEASE=true npm run build",
    "test": "cross-env ANGULARITYTODOES5_NO_APP=true npm run build && karma start",
    "ci": "cross-env ANGULARITYTODOES5_KARMA_REPORTER=teamcity npm run test"
  },
  "devDependencies": {
    "webpack-angularity-solution": "latest",
    "karma-angularity-solution": "latest"
  }
}
