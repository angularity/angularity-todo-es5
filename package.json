{
  "name": "angularity-todo-es5",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "node-bourbon": "1.2.2"
  },
  "bundledDependencies": [
    "examplelib"
  ],
  "scripts": {
    "build": "webpack -d --progress",
    "build-unminified": "cross-env NO_MINIFY=true webpack -d --progress",
    "watch": "webpack -d --watch",
    "watch-unminified": "cross-env NO_MINIFY=true webpack -d --watch",
    "release": "cross-env RELEASE=true webpack -d --progress",
    "test": "cross-env NO_APP=true webpack -d --progress && karma start",
    "ci": "cross-env KARMA_REPORTER=teamcity cross-env NO_APP=true webpack -d --progress && karma start"
  },
  "devDependencies": {
    "webpack-angularity-solution": "latest",
    "karma-angularity-solution": "latest"
  }
}
